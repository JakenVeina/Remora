<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Version>1.2.4</Version>
        <Description>A behaviour-based activity package for any application.</Description>
        <PackageReleaseNotes>
            Don't bubble up TaskCanceledExceptions.
        </PackageReleaseNotes>
        <PackageTags>$(PackageTags);service;behaviour;async;await;tpl</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.3" />
        <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="3.1.3" />
    </ItemGroup>

</Project>
