<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Version>3.0.4</Version>
        <Description>
            Base behaviour types for Discord.Net.Commands, using Remora.Behaviours.
        </Description>
        <PackageReleaseNotes>
            Add the SocketCommandContext to the available services.
        </PackageReleaseNotes>
        <PackageTags>$(PackageTags);discord;behaviours;commands</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\Remora.Discord.Behaviours\Remora.Discord.Behaviours.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Discord.Net.Commands" Version="2.1.1" />
    </ItemGroup>

</Project>
