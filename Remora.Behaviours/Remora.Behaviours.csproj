<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Version>1.2.0</Version>
        <Description>A behaviour-based activity package for any application.</Description>
        <PackageReleaseNotes>
            Add the DelayedAction behaviour and its associated service.
        </PackageReleaseNotes>
        <PackageTags>$(PackageTags);service;behaviour;async;await;tpl</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
        <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="2.2.0" />
    </ItemGroup>

</Project>
